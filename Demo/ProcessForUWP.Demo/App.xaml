<Application
    x:Class="ProcessForUWP.Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <muxc:XamlControlsResources />
                <!--  Add resource dictionaries here  -->
            </ResourceDictionary.MergedDictionaries>
            <!--  Add resources here  -->
            <CornerRadius x:Key="ControlCornerRadius">4</CornerRadius>
            <CornerRadius x:Key="OverlayCornerRadius">8</CornerRadius>
            <FontFamily x:Key="CodeThemeFontFamily">Cascadia Code NF,Cascadia Code PL,Cascadia Code,Consolas,Courier New,Liberation Mono,SFMono-Regular,Menlo,Monaco,monospace</FontFamily>
            <FontFamily x:Key="SymbolThemeFontFamily">Segoe Fluent Icons,Segoe MDL2 Assets,Segoe UI Symbol</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
